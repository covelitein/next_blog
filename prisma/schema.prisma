generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  img String
  content String
}



model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}